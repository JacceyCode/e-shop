{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://java-ecom-env.eba-xkarzmcq.us-east-1.elasticbeanstalk.com/api/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "http://java-ecom-env.eba-xkarzmcq.us-east-1.elasticbeanstalk.com/images/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/images/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000, immutable"
        }
      ]
    }
  ]
}
